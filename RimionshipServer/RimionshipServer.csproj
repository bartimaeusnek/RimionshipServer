		<PackageReference Include="Flurl.Http" Version="3.0.1" />
		<PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="5.0.2" />
		<PackageReference Include="Grpc.AspNetCore" Version="2.45.0" />
	</ItemGroup>

</Project>
