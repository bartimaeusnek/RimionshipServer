		<UserSecretsId>3ca70a3b-0c13-4dd1-a7f2-5f32d3e3875a</UserSecretsId>
		<StartupObject>RimionshipServer.Program</StartupObject>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
